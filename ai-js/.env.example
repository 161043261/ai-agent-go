# Server Configuration
HOST=0.0.0.0
PORT=9090
APP_NAME=GopherAI

# Database
DATABASE_URL="mysql://root:123456@localhost:3306/GopherAI"

# Redis (optional, set REDIS_ENABLED=false to use memory cache)
REDIS_ENABLED=true
REDIS_HOST=127.0.0.1
REDIS_PORT=6379
REDIS_PASSWORD=
REDIS_DB=0

# JWT Configuration
JWT_SECRET=GopherAI-v1
JWT_EXPIRES_IN=8760h
JWT_ISSUER=huanheart
JWT_SUBJECT=GopherAI

# RAG Model Configuration
RAG_EMBEDDING_MODEL=text-embedding-v4
RAG_CHAT_MODEL=qwen-turbo
RAG_DOC_DIR=./docs
RAG_BASE_URL=https://dashscope.aliyuncs.com/compatible-mode/v1
RAG_DIMENSION=1024

# OpenAI Compatible API
OPENAI_API_KEY=your-api-key
OPENAI_BASE_URL=https://dashscope.aliyuncs.com/compatible-mode/v1

# Image Recognition (optional)
IMAGE_ENABLED=true
ONNX_RUNTIME_LIB=../onnxruntime-win-x64-1.23.2/lib/onnxruntime.dll
IMAGE_MODEL_PATH=./models/mobilenetv2-7.onnx
IMAGE_LABEL_PATH=./models/synset.txt
